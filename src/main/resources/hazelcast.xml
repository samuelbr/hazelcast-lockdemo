           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<properties>
		<property name="hazelcast.logging.type">slf4j</property>
	</properties>
	
	<network>
		<join>
			<multicast enabled="true"/>
		</join>
	</network>
	
</hazelcast>